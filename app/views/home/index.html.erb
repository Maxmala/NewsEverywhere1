<h1>NEWS</h1>


	<table>
	

	<thead>
		<th>Nom de la source</th>
		<th>Auteur</th>
		<th>Titre</th>
		<th>Description</th>
	
	</thead>

	<tbody>
		<% @response_body['articles'].each do |article| %>
	<tr>
	<td><%= article["source"]['name'] %></td>
	<td><%= article["author"] %></td>
	<td><%= article["description"] %></td>
	<td><a href= "<%= article["url"] %>"> <img src="<%= article["urlToImage"] %>" alt="Image de l'article" > </a></td>
	<td></td>

</tr>
<% end %>


</tbody>
</table>






	






